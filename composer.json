{
    "name": "origami/money",
    "description": "Money handling and formatting package for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank Limited",
            "homepage": "https://papertank.com",
            "email": "hello@papertank.com"
        },
        {
            "name": "David Rushton",
            "email": "david@papertank.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "moneyphp/money": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Money\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Origami\\Money\\MoneyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
