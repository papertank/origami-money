{
    "name": "origami/money",
    "description": "Money handling and formatting package for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "hello@papertank.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-intl": "*",
        "illuminate/support": "^6.0|^7.0",
        "moneyphp/money": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Money\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Money\\MoneyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
